    "#Goal: read the file names into a list.\n",
    "import os\n",
    "import os.path\n",
    "import glob\n",
    "def read_filenames(filepath):\n",
    "    files = glob.glob(f'{filepath}/*.tif')\n",
    "    tags = []\n",
    "    for file in files:\n",
    "        tags.append(filename)\n",
    "    return tags"
   ]
